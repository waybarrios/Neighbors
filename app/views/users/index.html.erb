<h1>Listing users</h1>



<% @users.each do |user| %>

<ul class="vcard small-12 small-centered medium-9 medium-centered large-9 large-centered columns" columns large-3 medium-3 small-12> 
    <li class="fn">
      <h1 class=""><%= user.name %></h1>
    </li> 
    <li class="street-address">
      <%= user.birthday %>
    </li> 
    <li class="locality">                  
        <%= user.gender %>               
    </li>       
    <li class="street-address">
      <%= user.type_user %>
    </li> 
   
    <li class="street-address">
      <%= user.email %>
    </li> 
    <li class="street-address">
      <%= user.username %>
    </li> 

    <li class="email">
      <span><%= link_to 'Show', user, class: "" %></span> <span><%= link_to 'Edit', edit_user_path(user), class: "" %></span> <span><%= link_to 'Destroy', user, class: "",method: :delete, data: { confirm: 'Are you sure?' } %></span> 
    </li> 
  </ul>

<% end %>


<br />

<%= link_to 'New User', new_user_path, class: "button radius medium-5 small-12 medium-push-3 columns success"  %>
